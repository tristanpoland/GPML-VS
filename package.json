{
  "name": "gpml-vs",
  "displayName": "GPML Language Support",
  "description": "Language support for GPML files (.gpml) including syntax highlighting.",
  "version": "0.0.1",
  "icon": "branding/GPML_Brand.png",
  "main": "./out/extension.js",
  "activationEvents": [
    "onLanguage:gpml"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tristanpoland/GPML-VS"
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./"
  },
  "devDependencies": {
    "@types/node": "^20.19.17",
    "@types/vscode": "^1.70.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "vscode": ">=1.70.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "gpml",
        "aliases": [
          "GPML",
          "gpml"
        ],
        "extensions": [
          ".gpml"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "branding/GPML_Icon.png",
          "dark": "branding/GPML_Icon.png"
        }
      }
    ],
    "grammars": [
      {
        "language": "gpml",
        "scopeName": "source.gpml",
        "path": "./syntaxes/gpml.tmLanguage.json"
      }
    ]
  }
}
